pipeline {
    agent any
    tools {
        maven 'Maven'
    }
    stages{
        stage('Checkout'){
            steps{
                git branch: 'master', url:'https://github.com/Suresh8904/javatest.git'
            }
        }
        stage('Build'){
            steps{
                sh 'mvn clean install'
            }     
        }
        stage('Test'){
            steps {
                sh 'mvn test'
        }
    }
        stage('Deploy'){
             steps{
                echo 'Deploying the application'
        }
    }
}
